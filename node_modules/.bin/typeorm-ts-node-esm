#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/QA-QE  FULLSTACK/Interview/Server/node_modules/.pnpm/typeorm@0.3.21_pg@8.14.1_re_31dcd009e019387afdf843693859f909/node_modules/typeorm/node_modules:/mnt/c/QA-QE  FULLSTACK/Interview/Server/node_modules/.pnpm/typeorm@0.3.21_pg@8.14.1_re_31dcd009e019387afdf843693859f909/node_modules:/mnt/c/QA-QE  FULLSTACK/Interview/Server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/QA-QE  FULLSTACK/Interview/Server/node_modules/.pnpm/typeorm@0.3.21_pg@8.14.1_re_31dcd009e019387afdf843693859f909/node_modules/typeorm/node_modules:/mnt/c/QA-QE  FULLSTACK/Interview/Server/node_modules/.pnpm/typeorm@0.3.21_pg@8.14.1_re_31dcd009e019387afdf843693859f909/node_modules:/mnt/c/QA-QE  FULLSTACK/Interview/Server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
